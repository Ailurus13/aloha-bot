// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("PRISMA_DATABASE_URL")
}

model BotGuild {
  id             String   @id
  messageReactId String?
  createdAt      DateTime @default(now())
}

generator client {
  provider = "prisma-client-js"
}
